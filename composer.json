{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.0",
        "ext-json": "*",
        "acme/duplicate-field": "*",
        "acme/theme": "*",
        "ajcastro/eager-load-pivot-relations": "^0.2.2",
        "alfredo-ramos/parsedown-extra-laravel": "^4.0",
        "coderello/laravel-nova-lang": "^1.8",
        "drobee/nova-sluggable": "^1.2.7",
        "epartment/nova-dependency-container": "^1.3",
        "erusev/parsedown-extra": "^0.8.1",
        "fruitcake/laravel-cors": "^3.0",
        "gijsg/system-resources": "^1.0",
        "guzzlehttp/guzzle": "^7.4",
        "kabbouchi/nova-impersonate": "^1.14",
        "kabbouchi/nova-logs-tool": "^0.7.0",
        "laravel/framework": "^9.9.0",
        "laravel/nova": "^3.32.0",
        "laravel/tinker": "^2.7",
        "laravel/ui": "^3.4.5",
        "livewire/livewire": "^2.10",
        "manogi/nova-tiptap": "^2.8.2",
        "optimistdigital/nova-sortable": "^2.4.4",
        "saumini/count": "^1.0.1",
        "sbine/route-viewer": "^0.0.7",
        "spatie/laravel-activitylog": "^4.5.2",
        "spatie/laravel-backup": "^8.1.2",
        "spatie/laravel-cookie-consent": "^3.2.1",
        "spatie/laravel-permission": "^5.5.2",
        "spatie/laravel-query-builder": "^5.0.2",
        "spatie/laravel-tags": "^4.3.1",
        "spatie/nova-backup-tool": "^4.2.3",
        "spatie/nova-tags-field": "^3.4.5",
        "spatie/nova-tail-tool": "^3.0.2",
        "symfony/var-exporter": "^6.0.7",
        "tightenco/nova-google-analytics": "^2.0",
        "titasgailius/search-relations": "^2.0.1",
        "tonysm/tailwindcss-laravel": "^0.4.0",
        "vyuldashev/nova-money-field": "^1.4.0"
    },
    "require-dev": {
        "authanram/laravel-query-monitor": "^1.0",
        "barryvdh/laravel-ide-helper": "^2.12",
        "cloudstudio/resource-generator": "^1.5",
        "fakerphp/faker": "^1.9.1",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^6.1",
        "nunomaduro/phpinsights": "^2.2",
        "phpunit/phpunit": "^9.5.10",
        "roave/security-advisories": "dev-latest",
        "ryangjchandler/blade-tabler-icons": "^1.3",
        "spatie/laravel-ignition": "^1.0",
        "spatie/laravel-ray": "^1.29"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Systems\\Seed\\": "app/Systems/Seed/src/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        },
        "files": [
            "app/helpers.php"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "if [ $COMPOSER_DEV_MODE -eq 1 ]; then php artisan ide-helper:generate; fi",
            "if [ $COMPOSER_DEV_MODE -eq 1 ]; then php artisan ide-helper:meta; fi",
            "if [ $COMPOSER_DEV_MODE -eq 1 ]; then php artisan ide-helper:models -N; fi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "repositories": {
        "laravel/nova": {
            "type": "composer",
            "url": "https://nova.laravel.com"
        },
        "acme/duplicate-field": {
            "type": "path",
            "url": "./nova-components/DuplicateField"
        },
        "0": {
            "type": "path",
            "url": "./nova-components/Theme"
        },
        "1": {
            "type": "path",
            "url": "./nova-components/Theme"
        }
    }
}
